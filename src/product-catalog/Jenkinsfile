pipeline {
    agent any

        stages {
            stage ('Code Checkout') {
                steps {
                     // Clean workspace to avoid leftover files
                     cleanWs()
                     checkout scm
                     sh '''
                     echo "Commit SHA: $(git rev-parse HEAD)"
                     echo "Branch: $(git rev-parse --abbrev-ref HEAD)"
                     echo "Build number: ${BUILD_NUMBER}"
                  '''
                }
            }

            stage ('Build') {
                steps {
                    echo 'test'
                }
            }

            stage ('Test Test') {
                steps {
                    echo 'test test'
                }
            }
        }
}